<%= simple_form_for(@service) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :type %>
    <%= f.input :company %>
    <%= f.input :contact %>
    <%= f.input :phone %>
    <%= f.input :email %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :price_with_vat %>
    <%= f.input :payed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
